class Animal:
    def eat(self):
        print('------eat-------')
    def drink(self):
        print('------drink-------')
    def sleep(self):
        print('------sleep-------')
    def run(self):
        print('------run-------')


class Dog(Animal):

    def bark(self):
        print('-----wangwangwang-----')
class Cat(Animal):

    def catch(self):
        print('-----catch mouse------')

Teddy = Dog()

Teddy.eat()
Tom = Cat()

Tom.catch()
Tom.eat()
